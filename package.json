  "bugs": {
    "url": "https://github.com/andyleejordan/bicep-types-dsc/issues"
  },
  "homepage": "https://github.com/andyleejordan/bicep-types-dsc#readme",
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/node": "^20.17.30",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^10.1.2",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1"
  },
  "dependencies": {
    "@types/json-schema": "^7.0.15",
